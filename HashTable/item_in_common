def item_in_common(list1, list2):
    hash_table = {}
    for item in list1:
        hash_table[item] = True
    for item in list2:
        if item in hash_table:
            return True
    return False

if __name__ == "__main__":
    list_a = [1, 2, 3, 4, 5]
    list_b = [6, 7, 8, 9, 10]
    list_c = [5, 6, 7, 8, 9]
    
    print(item_in_common(list_a, list_b))  # Expected output: False
    print(item_in_common(list_a, list_c))  # Expected output: True
